<view class="container"> 
 
 <view class="login-icon"> 
   <image class="login-img" src="/images/logo.jpg" style="width: 558rpx; height: 563rpx; display: inline-block; box-sizing: border-box; left: NaNrpx; top: NaNrpx"></image> 
 </view> 
 <view class="login-from"> 

   <!--学号-->
   <view class="inputView"> 
     <image class="nameImage" src="/images/userid.png"></image> 
     <label class="loginLab">学号</label> 
     <input class="inputText" placeholder="请输入学号" bindinput="useridInput" /> 
   </view> 
   <view class="line"></view> 
   <!--密码-->
   <view class="inputView"> 
     <image class="keyImage" src="/images/password.png"></image> 
     <label class="loginLab">密码</label> 
     <input class="inputText" password="true" placeholder="请输入密码" bindinput="passwordInput" /> 
   </view> 
   <!--登陆-->
   <view class="loginBtnView" style="width: 619rpx; height: 106rpx; display: block; box-sizing: border-box; left: NaNrpx; top: NaNrpx"> 
     <button class="loginBtn" type="primary" size="{{primarySize}}" 
         loading="{{loading}}" plain="{{plain}}" disabled="{{disabled}}" bindtap="login" style="position: relative; left: -155rpx; top: -73rpx; width: 300rpx; height: 94rpx; display: block; box-sizing: border-box">登录</button> 
     <button class="loginBtn" type="primary" size="{{primarySize}}" 
         loading="{{loading}}" plain="{{plain}}" disabled="{{disabled}}" bindtap="login" style="position: relative; left: 158rpx; top: -249rpx; width: 300rpx; height: 94rpx; display: block; box-sizing: border-box;">管理员登录</button>
   </view>
   
    <!--注册-->
    <view class="registerBtnView"> 
      <button class="registerBtn" type="primary" size="{{primarySize}}" 
          loading="{{loading}}" plain="{{plain}}" disabled="{{disabled}}" bindtap="register">注册</button> 
    </view>


 </view> 

</view>

<view class = 'success' wx:if="{{success}}">
 <web-view src="{{text}}"></web-view>
</view>