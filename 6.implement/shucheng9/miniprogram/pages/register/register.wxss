page {
    height: 100%;
}

.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    padding: 0;
    box-sizing: border-box;
    background-color: #f2f2f2
}

.text {
    background-color: rgb(167, 167, 167);
    width: 100%;
    height: 80rpx;
    line-height: 20px;
}

.register-icon {
    flex: none;
}

/*表单内容*/
.register-from {
    margin-top: 20px;
    flex: auto;
    height: 100%;
    width: 90%; 
    height: 1222rpx; 
    display: block; 
    box-sizing: border-box; 
    left: 0rpx; 
    top: 0rpx
}

.inputview {
    background-color: #fff;
    line-height: 44px;
    height: 100rpx;
    
}

/*输入框*/
.nameimage,
.keyimage {
    margin-left: 20px;
    margin-top: 18px;
    width: 18px;
    height: 18px
}

.registerlab {
    margin:5% 5% 5% 5%;
    color: #545454;
    flex: block;
}

.inputtext {
    flex: block;
    float: right;
    text-align: left;
    margin-top: 5%;
    color: #cccccc;
}

.line {
    width: 100%;
    height: 1px;
    background-color: #cccccc;
    margin-top: 1px;
}

.registerbtnview {
    width: 100%;
    height: auto;
    background-color: #f2f2f2;
    margin-top: 0px;
    margin-bottom: 0px;
    padding-bottom: 0px;
}

.registerbtn {
    width: 100%;
    margin-top: 35px;
    font-size: 38rpx;
}
